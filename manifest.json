{
  "manifest_version": 3,
  "name": "TabLeaf",
  "version": "1.0",
  "description": "An extension that visually displays the items in the browser's favorites.",
  "permissions": ["bookmarks", "tabs"],
  "action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "48": "images/favicon-48x48.png",
      "128": "images/favicon-128x128.png"
    },
    "default_title": "TabLeaf"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "48": "images/favicon-48x48.png",
    "128": "images/favicon-128x128.png"
  },
  "safari": {
    "strict_min_version": "15.0",
    "strict_max_version": "17.0"
  }
}
